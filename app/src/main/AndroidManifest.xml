<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="py.com.chacomer.stock">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:networkSecurityConfig="@xml/network_security_config"
        android:name="py.com.chacomer.stock.Controller.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Activities.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.GetDocument"></activity>
        <activity android:name=".Activities.Main"></activity>
        <activity android:name=".Activities.AutomaticBarcode"></activity>
        <activity android:name=".Activities.CheckStock"></activity>
        <activity android:name=".Activities.GetOCData"></activity>
        <activity android:name=".Activities.CheckReceptionStock"></activity>
        <activity android:name=".Activities.CheckPedido"></activity>
        <activity android:name=".Activities.OpenCkStock"></activity>
        <activity android:name=".Activities.CheckLinealStock"></activity>
        <activity android:name=".Activities.CheckLocation"></activity>
        <activity android:name=".Activities.AssignEan"></activity>
    </application>
</manifest>
